{
    "name": "robertsaupe/phpbat",
    "description": "a PHP based Backup & Admin Tool",
    "keywords": [
        "phar",
        "php",
        "php8",
        "php-cli",
        "cli",
        "command-line",
        "console-application",
        "archive",
        "tar",
        "gz",
        "backup",
        "backups",
        "encrypted-backups",
        "encryption",
        "update",
        "upgrade",
        "clear",
        "log",
        "rsync",
        "ftp",
        "ftps",
        "sftp",
        "mysqldump"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Saupe",
            "email": "mail@robertsaupe.de",
            "homepage": "https://robertsaupe.de"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1",
        "ext-phar": "*",
        "ifsnop/mysqldump-php": "^2.9",
        "justinrainbow/json-schema": "^5.2",
        "laravel-zero/phar-updater": "^1.3",
        "phpmailer/phpmailer": "^6.5",
        "phpseclib/phpseclib": "^3.0",
        "seld/jsonlint": "^1.9",
        "splitbrain/php-archive": "^1.2",
        "symfony/console": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/process": "^6.0",
        "symfony/var-dumper": "^6.0",
        "webmozart/assert": "^1.10"
    },
    "autoload": {
        "psr-4": { 
            "robertsaupe\\phpbat\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "8.0.2"
        },
        "platform-check": false,
        "sort-packages": true
    },
    "bin": ["bin/phpbat"]
}
